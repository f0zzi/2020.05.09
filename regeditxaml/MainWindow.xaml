<Window x:Class="regeditxaml.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:regeditxaml"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="339.336">
    <StackPanel>
        <Button Content="Add" Click="Add"/>
        <ListBox x:Name="lbKeys" Height="350" ItemsSource="{Binding}"></ListBox>
        <Button Content="Delete" Click="Delete"/>
    </StackPanel>
</Window>
